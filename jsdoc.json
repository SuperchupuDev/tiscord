{
    "source": {
        "include": ["./src", "README.md"],
        "includePattern": ".(jsx|js|ts|tsx)$"
    },
    "plugins": [
        "plugins/markdown",
        "node_modules/better-docs/typescript"
    ],
    "opts": {
        "encoding": "utf8",
        "readme": "./README.md",
        "destination": "docs/",
        "recurse": true,
        "verbose": true,
        "template": "node_modules/better-docs"
        },
    "templates": {
        "search": true,
        "better-docs": {
            "name": "Tiscord",
            "logo": "assets/hcord.png",
            "title": "Tiscord documentation", 
            "css": "assets/style.css",
	        "hideGenerator": false,
            "navLinks": [
                {
                    "label": "Github",
                    "href": "https://github.com/xiboon/tiscord"
                },
                {
                    "label": "Support",
                    "href": "https://discord.gg/exUr7bjRjb"
                }
            ]
        }
    }
}