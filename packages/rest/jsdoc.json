{
    "source": {
        "include": [
            "./dist",
            "README.md"
        ],
        "includePattern": ".js$"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "encoding": "utf8",
        "readme": "./README.md",
        "destination": "../../docs/rest",
        "recurse": true,
        "verbose": true,
        "template": "./node_modules/clean-jsdoc-theme",
        "tutorials": "./md"
    }
}